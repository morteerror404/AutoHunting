{
    "nmap": {
        "open_ports": {
            "regex": "^(\\d+)\\/tcp\\s+open\\s+([^\\s]+)",
            "fields": ["port", "service"]
        },
        "os_detection": {
            "regex": "Running: (.*)",
            "fields": ["os_details"]
        },
        "service_version": {
            "regex": "^\\d+\\/tcp\\s+open\\s+([^\\s]+)\\s+(.*)",
            "fields": ["service", "version"]
        }
    },
    "subfinder": {
        "subdomains": {
            "regex": "^([a-zA-Z0-9.-]+\\.[a-zA-Z]{2,})$",
            "fields": ["subdomain"]
        }
    },
    "amass": {
        "subdomains": {
            "regex": "^([a-zA-Z0-9.-]+\\.[a-zA-Z]{2,})$",
            "fields": ["subdomain"]
        }
    },
    "assetfinder": {
        "subdomains": {
            "regex": "^([a-zA-Z0-9.-]+\\.[a-zA-Z]{2,})$",
            "fields": ["subdomain"]
        }
    },
    "katana": {
        "endpoints": {
            "regex": "^(https?://[^\\s]+)$",
            "fields": ["url"]
        }
    },
    "httpx": {
        "live_hosts": {
            "regex": "^(https?://[^ ]+) \\[(\\d{3})\\](?: \\[([^\\]]*)\\])?",
            "fields": ["url", "status_code", "title"]
        },
        "technologies": {
            "regex": "^(https?://[^ ]+).*?\\[([^\\]]+)\\]$",
            "fields": ["url", "technologies"]
        }
    },
    "gau": {
        "endpoints": {
            "regex": "^(https?://[^\\s]+)$",
            "fields": ["url"]
        }
    },
    "waybackurls": {
        "endpoints": {
            "regex": "^(https?://[^\\s]+)$",
            "fields": ["url"]
        }
    },
    "awsbucketdump": {
        "s3_buckets": {
            "regex": "(http?://\\S+\\.s3\\.\\S+amazonaws.com\\S*)",
            "fields": ["bucket_url"]
        }
    },
    "nuclei": {
        "vulnerabilities": {
            "regex": "^\\[([^\\]]+)\\] \\[([^\\]]+)\\] \\[([^\\]]+)\\] (.*)$",
            "fields": ["template_id", "protocol", "severity", "url"]
        }
    },
    "ffuf": {
        "endpoints": {
            "regex": "^(https?://[^\\s]+)$", 
            "fields": ["url"]
        }
    },
    "gobuster": {
        "endpoints": {
            "regex": "^(https?://[^\\s]+) \\(Status: (\\d{3})\\)",
            "fields": ["url", "status_code"]
        }
    },
    "nikto": {
        "findings": {
            "regex": "^\\+ (.*)$",
            "fields": ["finding_description"]
        }
    },
    "sqlmap": {
        "vulnerable_params": {
            "regex": "parameter '([^']+)' is vulnerable",
            "fields": ["vulnerable_parameter"]
        }
    },
    "gitleaks": {
        "secrets_found": {
            "regex": "^\\[(.*?)\\] (.*?) in (.*)",
            "fields": ["severity", "rule_id", "file_path"]
        }
    },
    "wafw00f": {
        "waf_detected": {
            "regex": "is behind a (.*) WAF\\.",
            "fields": ["waf_name"]
        }
    },
    "dalfox": {
        "xss_found": {
            "regex": "^\\[FOUND\\] (.*)",
            "fields": ["vulnerable_url"]
        }
    },
    "linkfinder": {
        "endpoints": {
            "regex": "^(https?://[^\\s]+)$",
            "fields": ["url"]
        }
    },
    "kiterunner": {
        "endpoints": {
            "regex": "^\\d{3}\\s+\\d+B\\s+(https?://.+)$",
            "fields": ["url"]
        }
    },
    "default": {
        "full_line": {
            "regex": "^(.*)$",
            "fields": ["line_content"]
        }
    }
}