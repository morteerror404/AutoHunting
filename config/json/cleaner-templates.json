{
    "templates": {
        "nmap": "/etc/AutoHunting/templates/cleaner/nmap.json",
        "subfinder": "/etc/AutoHunting/templates/cleaner/subfinder.json",
        "amass": "/etc/AutoHunting/templates/cleaner/amass.json",
        "assetfinder": "/etc/AutoHunting/templates/cleaner/assetfinder.json",
        "katana": "/etc/AutoHunting/templates/cleaner/katana.json",
        "httpx": "/etc/AutoHunting/templates/cleaner/httpx.json",
        "gau": "/etc/AutoHunting/templates/cleaner/gau.json",
        "waybackurls": "/etc/AutoHunting/templates/cleaner/waybackurls.json",
        "reconftw": "/etc/AutoHunting/templates/cleaner/reconftw.json",
        "bugbountytoolkit": "/etc/AutoHunting/templates/cleaner/bugbountytoolkit.json",
        "awsbucketdump": "/etc/AutoHunting/templates/cleaner/awsbucketdump.json",
        "nuclei": "/etc/AutoHunting/templates/cleaner/nuclei.json",
        "ffuf": "/etc/AutoHunting/templates/cleaner/ffuf.json",
        "gobuster": "/etc/AutoHunting/templates/cleaner/gobuster.json",
        "nikto": "/etc/AutoHunting/templates/cleaner/nikto.json",
        "sqlmap": "/etc/AutoHunting/templates/cleaner/sqlmap.json",
        "wafw00f": "/etc/AutoHunting/templates/cleaner/wafw00f.json",
        "dalfox": "/etc/AutoHunting/templates/cleaner/dalfox.json",
        "xsstrike": "/etc/AutoHunting/templates/cleaner/xsstrike.json",
        "gopherus": "/etc/AutoHunting/templates/cleaner/gopherus.json",
        "lfisuite": "/etc/AutoHunting/templates/cleaner/lfisuite.json",
        "fimap": "/etc/AutoHunting/templates/cleaner/fimap.json",
        "oralyzer": "/etc/AutoHunting/templates/cleaner/oralyzer.json",
        "cmseek": "/etc/AutoHunting/templates/cleaner/cmseek.json",
        "kiterunner": "/etc/AutoHunting/templates/cleaner/kiterunner.json",
        "aquatone": "/etc/AutoHunting/templates/cleaner/aquatone.json",
        "eyewitness": "/etc/AutoHunting/templates/cleaner/eyewitness.json",
        "linkfinder": "/etc/AutoHunting/templates/cleaner/linkfinder.json",
        "gf": "/etc/AutoHunting/templates/cleaner/gf.json",
        "gitdumper": "/etc/AutoHunting/templates/cleaner/gitdumper.json",
        "metasploit": "/etc/AutoHunting/templates/cleaner/metasploit.json"
    },
    "preconfigured_templates": {
        "nmap": {
            "open_ports": {
                "regex": "^(\\d+)\\/tcp\\s+open\\s+([^\\s]+)",
                "fields": ["port", "service"]
            }
        },
        "subfinder": {
            "subdomains": {
                "regex": "^([a-zA-Z0-9.-]+\\.[a-zA-Z]{2,})$",
                "fields": ["subdomain"]
            }
        },
        "amass": {
            "subdomains": {
                "regex": "^([a-zA-Z0-9.-]+\\.[a-zA-Z]{2,})$",
                "fields": ["subdomain"]
            }
        },
        "assetfinder": {
            "subdomains": {
                "regex": "^([a-zA-Z0-9.-]+\\.[a-zA-Z]{2,})$",
                "fields": ["subdomain"]
            }
        },
        "katana": {
            "endpoints": {
                "regex": "^(https?://[^\\s]+)$",
                "fields": ["url"]
            }
        },
        "httpx": {
            "live_hosts": {
                "regex": "^(https?://[^ ]+) \\[(\\d{3})\\](?: \\[([^\\]]*)\\])?",
                "fields": ["url", "status_code", "title"]
            }
        },
        "gau": {
            "endpoints": {
                "regex": "^(https?://[^\\s]+)$",
                "fields": ["url"]
            }
        },
        "waybackurls": {
            "endpoints": {
                "regex": "^(https?://[^\\s]+)$",
                "fields": ["url"]
            }
        },
        "awsbucketdump": {
            "s3_buckets": {
                "regex": "(http?://\\S+\\.s3\\.\\S+amazonaws.com\\S*)",
                "fields": ["bucket_url"]
            }
        },
        "nuclei": {
            "vulnerabilities": {
                "regex": "^\\[([^\\]]+)\\] \\[([^\\]]+)\\] \\[([^\\]]+)\\] (.*)$",
                "fields": ["template_id", "protocol", "severity", "url"]
            }
        },
        "ffuf": {
            "endpoints": {
                "regex": "^(https?://[^\\s]+)$", 
                "fields": ["url"]
            }
        },
        "gobuster": {
            "endpoints": {
                "regex": "^(https?://[^\\s]+) \\(Status: (\\d{3})\\)",
                "fields": ["url", "status_code"]
            }
        },
        "nikto": {
            "findings": {
                "regex": "^\\+ (.*)$",
                "fields": ["finding_description"]
            }
        },
        "sqlmap": {
            "vulnerable_params": {
                "regex": "parameter '([^']+)' is vulnerable",
                "fields": ["vulnerable_parameter"]
            }
        },
        "wafw00f": {
            "waf_detected": {
                "regex": "is behind a (.*) WAF\\.",
                "fields": ["waf_name"]
            }
        },
        "dalfox": {
            "xss_found": {
                "regex": "^\\[FOUND\\] (.*)",
                "fields": ["vulnerable_url"]
            }
        },
        "linkfinder": {
            "endpoints": {
                "regex": "^(https?://[^\\s]+)$",
                "fields": ["url"]
            }
        },
        "kiterunner": {
            "endpoints": {
                "regex": "^\\d{3}\\s+\\d+B\\s+(https?://.+)$",
                "fields": ["url"]
            }
        },
        "default": {
            "full_line": {
                "regex": "^(.*)$",
                "fields": ["line_content"]
            }
        }
    }
}